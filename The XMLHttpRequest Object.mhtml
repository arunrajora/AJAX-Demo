  void               <a href=3D"https://www.w3.org/TR/2006/WD-XMLHttpReques=
t-20060405/#dfn-open">open</a>(in DOMString method, in DOMString uri, in bo=
olean async);
  void               <a href=3D"https://www.w3.org/TR/2006/WD-XMLHttpReques=
t-20060405/#dfn-open">open</a>(in DOMString method, in DOMString uri, in bo=
olean async, in DOMString user);
  void               <a href=3D"https://www.w3.org/TR/2006/WD-XMLHttpReques=
t-20060405/#dfn-open">open</a>(in DOMString method, in DOMString uri, in bo=
